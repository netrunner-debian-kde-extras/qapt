msgctxt "@title:window"
msgid "Package Not Found"
msgstr "Pacchetto non trovato"

#: utils/qapt-gst-helper/PluginHelper.cpp:331
msgctxt "@title:window"
msgid "Media Change Required"
msgstr "È necessario un cambio del supporto"

